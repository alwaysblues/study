<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>jspstudy</display-name>
  <welcome-file-list>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>InitParamEx</servlet-name>
    <servlet-class>ex.initparam.InitParamEx</servlet-class>
    <init-param>
      <param-name>id</param-name>
      <param-value>kimtaekook</param-value>
    </init-param>
    <init-param>
      <param-name>pw</param-name>
      <param-value>a1b2c3d4</param-value>
    </init-param>
    <init-param>
      <param-name>path</param-name>
      <param-value>d:\</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitParamEx</servlet-name>
    <url-pattern>/init</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>ex.contextlistener.ContextListenerEx</listener-class>
  </listener>
  <context-param>
    <param-name>id</param-name>
    <param-value>kimtaekook</param-value>
  </context-param>
  <context-param>
    <param-name>pw</param-name>
    <param-value>1234567890</param-value>
  </context-param>
  <context-param>
    <param-name>path</param-name>
    <param-value>d:\</param-value>
  </context-param>
  <error-page>
    <error-code>404</error-code>
    <location>/0130/jsptag/error404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/0130/jsptag/error500.jsp</location>
  </error-page>
</web-app>